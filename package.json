    "inject-html": "^1.1.2",
    "jasmine-core": "^3.5.0",
    "jsdoc": "^3.6.3",
    "karma": "^4.3.0",
    "karma-browserstack-launcher": "^1.5.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage": "^2.0.1",
